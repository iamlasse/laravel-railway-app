{
    "name": "iamlasse/telekomkollen",
    "type": "project",
    "description": "TK Dashboard",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.1",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "blade-ui-kit/blade-icons": "^1.1",
        "calebporzio/parental": "^1.0",
        "coconutcraig/laravel-postmark": "^3.0",
        "codeat3/blade-eos-icons": "^1.11.2",
        "doctrine/dbal": "^3.2",
        "filament/forms": "^2.0",
        "fruitcake/laravel-cors": "^2.0",
        "grosv/laravel-passwordless-login": "^1.6",
        "guzzlehttp/guzzle": "^7.4",
        "larabug/larabug": "^2.5",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.4",
        "laravel/sanctum": "^2.11",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.5",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-login-link": "^1.1",
        "spatie/laravel-permission": "^5.4",
        "symfony/filesystem": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/postmark-mailer": "^6.0",
        "wire-elements/modal": "^1.0.4",
        "wireui/wireui": "^1.15"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "brianium/paratest": "^6.4",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.4",
        "itsgoingd/clockwork": "^5.1",
        "kamona/kui-laravel-jetstream": "^0.1.0",
        "laradumps/laradumps": "^1.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^1.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Telekom\\": "telekom/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan telescope:publish --ansi",
            "@php artisan optimize"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope",
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
